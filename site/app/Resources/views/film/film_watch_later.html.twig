{% extends 'base.html.twig' %}

{% block body %}
    {% if filmsWait is not empty  %}
        <h1> Votre liste d'attente </h1>
        <ul class="list-films list-films-page">
            {% for film in filmsWait %}
                {% include 'include/film_block.html.twig' %}
            {% endfor %}
        </ul>
    {% else %}
        <h1> Pas encore film dans votre liste d'attente </h1>
    {% endif %}
{% endblock %}

